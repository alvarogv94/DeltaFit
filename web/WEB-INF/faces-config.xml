<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>controlador.Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>registro</managed-bean-name>
        <managed-bean-class>controlador.Registro</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/perfil.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>no</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/registro.jsp</from-view-id>
        <navigation-case>
            <from-outcome>no</from-outcome>
            <to-view-id>/registro.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>solicitudPreparador</managed-bean-name>
        <managed-bean-class>controlador.solicitudPreparador.SolicitudPreparador</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/solicitud_preparador.jsp</from-view-id>
        <navigation-case>
            <from-outcome>no</from-outcome>
            <to-view-id>/solicitud_preparador.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/solicitud_preparador.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>graficoAtleta</managed-bean-name>
        <managed-bean-class>controlador.GraficoAtleta</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
