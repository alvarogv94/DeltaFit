<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>login</managed-bean-name>
        <managed-bean-class>controlador.Login</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>registro</managed-bean-name>
        <managed-bean-class>controlador.Registro</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>no</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/registro.jsp</from-view-id>
        <navigation-case>
            <from-outcome>no</from-outcome>
            <to-view-id>/registro.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>solicitudPreparador</managed-bean-name>
        <managed-bean-class>controlador.solicitudPreparador.SolicitudPreparador</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/solicitud_preparador.jsp</from-view-id>
        <navigation-case>
            <from-outcome>no</from-outcome>
            <to-view-id>/solicitud_preparador.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/solicitud_preparador.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>graficoAtleta</managed-bean-name>
        <managed-bean-class>controlador.GraficoAtleta</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RealizaPreparacion</managed-bean-name>
        <managed-bean-class>controlador.RealizaPreparacion</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>seleccionaAtleta</managed-bean-name>
        <managed-bean-class>controlador.SeleccionaAtleta</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/preparador/inicio.jsp</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/preparador/preparacion.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>subeImagenPerfil</managed-bean-name>
        <managed-bean-class>controlador.SubeImagenPerfil</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>subeImagenPerfil1</managed-bean-name>
        <managed-bean-class>controlador.SubeImagenPerfil</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>fileUploadBean</managed-bean-name>
        <managed-bean-class>controlador.FileUploadBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>editorUsuarioFoto</managed-bean-name>
        <managed-bean-class>controlador.EditorUsuarioFotoBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>fileUploadBean1</managed-bean-name>
        <managed-bean-class>controlador.FileUploadBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/preparador/preparacion.jsp</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/preparador/preparacion.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vuelve</from-outcome>
            <to-view-id>/preparador/inicio.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>chat</managed-bean-name>
        <managed-bean-class>controlador.Chat</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>seleccionaChat</managed-bean-name>
        <managed-bean-class>controlador.SeleccionaChat</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/preparador/chat.jsp</from-view-id>
        <navigation-case>
            <from-outcome>ok</from-outcome>
            <to-view-id>/preparador/conversacion.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>exportarPlan</managed-bean-name>
        <managed-bean-class>controlador.ExportarPlan</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>seleccionaPlanPredeterminado</managed-bean-name>
        <managed-bean-class>controlador.SeleccionaPlanPredeterminado</managed-bean-class>
    
        <managed-bean-scope>session</managed-bean-scope></managed-bean>
    <managed-bean>
        <managed-bean-name>modificaDatosAtleta</managed-bean-name>
        <managed-bean-class>controlador.ModificaDatosAtleta</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>modificaDatosPreparador</managed-bean-name>
        <managed-bean-class>controlador.ModificaDatosPreparador</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>datosTablas</managed-bean-name>
        <managed-bean-class>controlador.admin.DatosTablas</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>altaPlanPred</managed-bean-name>
        <managed-bean-class>controlador.admin.AltaPlanPred</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/admin/inicio.jsp</from-view-id>
        <navigation-case>
            <from-outcome>vuelve</from-outcome>
            <to-view-id>/admin/inicio.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>addNuevoEjercicio</managed-bean-name>
        <managed-bean-class>controlador.admin.AddNuevoEjercicio</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>realizaPago</managed-bean-name>
        <managed-bean-class>controlador.RealizaPago</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
